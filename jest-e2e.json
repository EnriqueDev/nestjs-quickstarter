{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "./src",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": ["ts-jest"]
  },
  "setupFiles": ["dotenv/config"],
  "setupFilesAfterEnv": ["<rootDir>/../testSetup.ts"],
  "moduleNameMapper": {
    "^~(.*)$": "<rootDir>/$1"
  }
}
